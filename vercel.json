{
    "builds": [
      {
        "src": "myproject/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",     
        "dest": "static/$1",     
        "headers": {
          "source": "/static/(.*)",   
          "headers": [
            {
              "key": "Content-Type",
              "value": "text/$1"      
            }
          ]
        }
      },
      {
        "src": "/(.*)",
        "dest": "myproject/wsgi.py"
      }
    ]
  }
  